{
  "extends": "./tsconfig-esm2015.json",
  "compilerOptions": {
    "target": "es5",
    "outDir": "../tmp/esm5",
    "rootDir": "../lib"
  },
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    "flatModuleOutFile": "ivy-rest.js",
    "flatModuleId": "ivy-rest"
  }
}
